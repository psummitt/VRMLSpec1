  Separator {	# Wall geometry; just three flat polygons
    Transform {
      scaleFactor  8.00   1.00   8.00
      }
    Coordinate3 {
      point[
         64 64 64,  64 64 -64,  64 -64 64,  64 -64 -64,
        -64 64 64, -64 64 -64, -64 -64 64, -64 -64 -64]
      }
    Separator {
      Material {
        ambientColor 3 3 3
        diffuseColor 0 0 0
        shininess	0
        }
      IndexedFaceSet {
        coordIndex[4, 5, 1, 0, -1]
        }
      }
    Separator {
      Material {
        ambientColor 0 0 0
        diffuseColor 0 0 0
        shininess	1
        }
      IndexedFaceSet {
        coordIndex[2, 3, 7, 6, -1]
        }
      }
